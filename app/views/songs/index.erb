<tbody>
  <% @songs.each do |song| %>
    <tr>
      <%= song.title %>
      <%= song.release_year %>
      <%= song.released %>
      <%= song.genre %>
      <%= song.artist_name %>
      <%= link_to "Delete #{song.title}", song, method: :delete, data: {confirm: "you sure?"} %>
    </tr>
  <% end %>
</tbody>
